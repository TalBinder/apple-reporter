{
  "name": "lightsub-apple-reporter",
  "version": "1.0.0",
  "description": "Promise-based Apple iTunes Connect Reporter - LightSub version",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel lib --out-dir dist",
    "check": "npm run lint && npm run test",
    "clean": "rimraf dist",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint lib",
    "prepublishOnly": "npm run check",
    "test": "nyc mocha -R spec --compilers js:babel-register -- test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/beardon/apple-reporter.git"
  },
  "keywords": [
    "apple",
    "autoingest",
    "autoingestion",
    "itc",
    "itunes-connect",
    "itunes",
    "promise",
    "reporter"
  ],
  "author": "Tyson Cleary <amec@tyscorp.net> (https://github.com/tyscorp)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beardon/apple-reporter/issues"
  },
  "homepage": "https://github.com/beardon/apple-reporter",
  "engine": {
    "node": ">= 4.2.0"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "es6-error": "^4.0.0",
    "lodash": "^4.16.4",
    "node-fetch": "^2.1.2",
    "stream-to-promise": "^2.2.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.18.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.18.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-beardon": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6"
  }
}
